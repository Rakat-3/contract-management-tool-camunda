      "layout": {
        "row": "Row_6",
        "columns": null
      },
      "conditional": {
        "hide": "=providersName == null"
      }
    },
    {
      "label": "Provider's Budget Offer",
      "type": "number",
      "id": "Field_ProvidersBudget",
      "key": "providersBudget",
      "disabled": true,
      "layout": {
        "row": "Row_7",
        "columns": null
      },
      "conditional": {
        "hide": "=providersName == null"
